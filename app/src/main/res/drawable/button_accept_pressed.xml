<?xml version="1.0" encoding="utf-8"?>
<!-- Unselected means the set button doesn't have a number in it but isn't inactive - aka the set has been skipped -->
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <corners
        android:bottomRightRadius="@dimen/button_friend_request_radius"
        android:topRightRadius="@dimen/button_friend_request_radius"
        android:bottomLeftRadius="@dimen/button_friend_request_radius"
        android:topLeftRadius="@dimen/button_friend_request_radius"/>

    <solid android:color="@color/colorPigeon" />

    <stroke
        android:width="1dp"
        android:color="@color/colorPigeon" />

</shape>